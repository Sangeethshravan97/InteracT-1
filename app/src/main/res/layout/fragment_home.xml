<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container1"
    android:background="@drawable/side_nav_bar"
    android:layout_height="match_parent"
    tools:context="com.example.rams.interact.home">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="vertical">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
    <android.support.v7.widget.CardView
        android:id="@+id/MVA"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="10dp"
        app:cardCornerRadius="15dp"
        app:cardBackgroundColor="#99000000"
        app:cardElevation="0dp">
    <!--MISSION-->
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="15dp"
            android:paddingBottom="15dp">
            <TextView
                android:id="@+id/Mission"
                android:layout_margin="5dp"
                android:textSize="20sp"
                android:textStyle="bold"
                android:textColor="#ffffff"
                android:layout_centerHorizontal="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Mission"
                android:textAlignment="center"
                />
            <TextView
                android:id="@+id/Mission1"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:layout_below="@id/Mission"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="1.Impart sound knowledge of IT domain to the Students."/>
            <TextView
                android:id="@+id/Mission2"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:layout_below="@id/Mission1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="2.Nuture students to contribute to dynamic industrial needs."/>
            <TextView
                android:id="@+id/Mission3"
                android:textColor="#ffffff"
                android:textSize="15sp"
                android:layout_below="@id/Mission2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="3.Empower faculty with the knownlegde in the emerging areas of IT"/>
        </RelativeLayout>
    </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/Vision"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="15dp"
                android:layout_below="@id/MVA"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="10dp"
                app:cardBackgroundColor="#99000000"
                app:cardElevation="0dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="15dp"
                    android:paddingBottom="15dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/VisionT"
                        android:text="Vision"
                        android:layout_centerHorizontal="true"
                        android:textAlignment="center"
                        android:textColor="#ffffff"
                        android:textSize="20sp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/Vision1"
                        android:layout_below="@id/VisionT"
                        android:textColor="#ffffff"
                        android:textSize="15sp"
                        android:text="To be an outstanding center of IT education and research for betterment of society."/>
                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/About"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="10dp"
                android:layout_below="@id/Vision"
                app:cardBackgroundColor="#99000000"
                app:cardElevation="0dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingLeft="15dp"
                    android:paddingBottom="15dp"
                    android:paddingRight="15dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/AboutT"
                        android:text="About"
                        android:layout_centerHorizontal="true"
                        android:textAlignment="center"
                        android:textColor="#ffffff"
                        android:textSize="20sp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/About1"
                        android:layout_below="@id/AboutT"
                        android:textColor="#ffffff"
                        android:textSize="15sp"
                        android:text="The department of Information Technology was established in the year 1999. Our mission is to provide quality education to the students and impart IT excellence in them. Apart from making them good technocrats, we also provide individual attention to make them good citizens of our nations to serve the industry and society constructively."/>
                </RelativeLayout>
            </android.support.v7.widget.CardView>

      <!--FACULTIY-->
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/dp1"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignParentLeft="true"
                android:src="@drawable/hod"
                android:layout_below="@id/About"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:elevation="10dp"
                />
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:id="@+id/HOD"
                android:layout_marginTop="10dp"
                android:layout_below="@id/About"
                android:layout_toRightOf="@id/dp1"
                app:cardBackgroundColor="#99000000"
                android:layout_marginLeft="-45dp"
                app:cardElevation="0dp"
                app:cardCornerRadius="15dp"
                android:layout_marginRight="15dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="50dp"
                    android:layout_marginTop="15dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="22sp"
                    android:textColor="#ffffff"
                    android:layout_centerHorizontal="true"
                    android:text="Dr. T. Nagarajan"
                    android:id="@+id/hodname"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/hoddes"
                        android:layout_below="@id/hodname"
                        android:textSize="15sp"
                        android:layout_centerHorizontal="true"
                        android:text="Head of Department and Professor"
                        android:textColor="#ffffff"/>
                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/dp2"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignParentLeft="true"
                android:src="@drawable/it_srinivasan"
                android:layout_below="@id/HOD"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:elevation="10dp"
                />
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:id="@+id/Srini"
                android:layout_marginTop="10dp"
                android:layout_below="@id/HOD"
                android:layout_toRightOf="@id/dp1"
                app:cardBackgroundColor="#99000000"
                android:layout_marginLeft="-45dp"
                app:cardCornerRadius="15dp"
                android:layout_marginRight="15dp"
                app:cardElevation="0dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="50dp"
                    android:layout_marginTop="15dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="#ffffff"
                        android:layout_centerHorizontal="true"
                        android:text="Dr. R. Srinivasan"
                        android:id="@+id/srininame"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/srinides"
                        android:layout_below="@id/srininame"
                        android:textSize="15sp"
                        android:layout_centerHorizontal="true"
                        android:text="Professor "
                        android:textColor="#ffffff"/>
                </RelativeLayout>
            </android.support.v7.widget.CardView>
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/dp3"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignParentLeft="true"
                android:src="@drawable/shahina"
                android:layout_below="@id/Srini"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:elevation="10dp"
                />
            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:id="@+id/Shahina"
                android:layout_marginTop="10dp"
                app:cardElevation="0dp"
                android:layout_below="@id/Srini"
                android:layout_toRightOf="@id/dp1"
                app:cardBackgroundColor="#99000000"
                android:layout_marginLeft="-45dp"
                app:cardCornerRadius="15dp"
                android:layout_marginRight="15dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="50dp"
                    android:layout_marginTop="15dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="22sp"
                        android:textColor="#ffffff"
                        android:layout_centerHorizontal="true"
                        android:text="Dr. A. Shahina"
                        android:id="@+id/shahinaname"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/shahinades"
                        android:layout_below="@id/shahinaname"
                        android:textSize="15sp"
                        android:layout_centerHorizontal="true"
                        android:text="Professor"
                        android:textColor="#ffffff"/>
                </RelativeLayout>
            </android.support.v7.widget.CardView>
            </RelativeLayout>
    </ScrollView>
</FrameLayout>
